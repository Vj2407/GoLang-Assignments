package main

import (
	"fmt"
)

var numbers = []int{0, 1, 2, 3, 4, 6, 8, 9}
var target int = 9

func Add(numbers []int, target int) (int, int) {
	if len(numbers) <= 1 {
		return 0, 0
	}
	vk := make(map[int]int)
	for i := 1; i < len(numbers); i++ {
		if val, ok := vk[numbers[i+1]]; ok {
			return val, i + 1
		} else {
			vk[target-numbers[i+1]] = i + 1
		}
	}
	return 0, 0
}
func main() {
	fmt.Println(Add(numbers, target))
}
